{
    "name": "@fivethree/billy-dev-kit",
    "version": "0.8.1",
    "description": "dev kit for billy.",
    "main": "dist/generated/index.js",
    "scripts": {
        "build": "node_modules/.bin/tsc -p .",
        "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/generated/index.ts",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
        "test": "npm i -g && billy-dev"
    },
    "bin": {
        "billy-dev": "dist/generated/index.js"
    },
    "preferGlobal": true,
    "keywords": [
        "typescript",
        "cli",
        "node",
        "terminal"
    ],
    "author": "garygrossgarten",
    "license": "MIT",
    "devDependencies": {
        "axios": "^0.18.0",
        "@types/node": "^10.12.18",
        "nodemon": "^1.18.9",
        "ts-node": "^7.0.1",
        "typescript": "^3.2.2"
    },
    "dependencies": {
        "@fivethree/billy-core": "0.8.1",
        "@fivethree/billy-plugin-core": "0.8.1"
    },
    "billy": {
        "plugins": [
            "@fivethree/billy-plugin-core"
        ]
    }
}
